/*----------------------------------------*\
  ELEMENTS
\*----------------------------------------*/

html {
  @apply overflow-y-scroll;
  @apply font-sans;
}

a {
  @apply text-red-500 underline
}

h1 {
  @apply text-h1;
}

h2 {
  @apply text-h2;
}

h3 {
  @apply text-h3;
}

h4 {
  @apply text-h4;
}

h5 {
  @apply text-h5;
}

h6 {
  @apply text-h6;
}
